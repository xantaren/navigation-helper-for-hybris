{
  "manifest_version": 3,
  "name": "Site Navigation Helper",
  "version": "1.0",
  "description": "Provides quick navigation options for supported sites",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
